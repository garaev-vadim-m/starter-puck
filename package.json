{
  "name": "Build use Client Solutions Practice Center",
  "author": "Garaev.VM@gazprom-neft.ru && Kollert.LV@gazprom-neft.ru",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && node build.js ",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --config .prettierrc.json src/ --write",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "husky install",
    "test": "npx cypress run",
    "test:ui": "npx cypress open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --config .prettierrc.json src/ --write"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.config.ts"
    }
  },
  "dependencies": {
    "axios": "^1.6.7",
    "element-plus": "^2.5.3",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@testing-library/vue": "^8.0.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.7",
    "@types/vue-the-mask": "^0.11.5",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.7.2",
    "cypress": "^13.6.4",
    "cz-customizable": "^7.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^8.0.3",
    "prettier": "^3.2.4",
    "terser": "^5.27.0",
    "typescript": "~5.3.3",
    "vite": "^4.5.2",
    "vue-tsc": "^1.8.27"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
