    "husky": "^8.0.3",
    "prettier": "^3.2.4",
    "terser": "^5.27.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitepress": "^1.0.0-rc.33",
    "vue-tsc": "^1.8.27"
